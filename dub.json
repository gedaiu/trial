      "sourcePaths": [
        "lifecycle",
        "tests"
      ],
      "stringImportPaths": [
        "lifecycle/trial"
      ],
      "importPaths": [
        "lifecycle",
        "tests"
      ],
      "dependencies": {
        "fluent-asserts": ">=0.12.0",
        "libdparse": ">=0.8.0"
      }
    },
    {
      "name": "unittest",
      "excludedSourceFiles": [
        "source/app.d",
        "runner/app.d"
      ],
      "sourcePaths": [
        "lifecycle",
        "runner",
        "tests"
      ],
      "stringImportPaths": [
        "lifecycle/trial"
      ],
      "importPaths": [
        "lifecycle",
        "runner",
        "tests"
      ],
      "dependencies": {
        "dub": "~>1.22.0",
        "fluent-asserts": ">=0.12.0",
        "libdparse": {
          "version": ">=0.8.0",
          "optional": true
        },
        "vibe-d:data": "~>0.9.0"
      },
      "subConfigurations": {
        "dub": "library-nonet"
      }
    }
  ],
  "subPackages": [
    {
      "name": "lifecycle",
      "targetType": "library",
      "sourcePaths": [
        "lifecycle"
      ],
      "importPaths": [
        "lifecycle"
