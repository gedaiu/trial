      }
    },
    {
      "name": "runner",
      "targetType": "executable",
      "targetName": "trial",
      "sourcePaths": [
        "runner",
        "lifecycle"
      ],
      "importPaths": [
        "runner",
        "lifecycle"
      ],
      "stringImportPaths": [
        "views",
        "lifecycle"
      ],
      "dependencies": {
        "dub": "~>1.22.0",
        "vibe-d:http": "~>0.9.0",
        "vibe-d:data": "~>0.9.0",
        "libdparse": "~>0.8.7",
