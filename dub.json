{
  "name": "trial",
  "authors": [
    "Szabo Bogdan"
  ],
  "description": "The extensible test runner for DLang",
  "copyright": "Copyright Â© 2022, Szabo Bogdan",
  "license": "MIT",
  "homepage": "http://trial.szabobogdan.com/",
  "dependencies": {},
  "configurations": [
    {
      "name": "library",
      "sourcePaths": [
        "lifecycle"
      ],
      "targetType": "sourceLibrary",
      "dependencies": {
        "fluent-asserts": "~>1.0.0",
        "libdparse": {
          "version": ">=0.20.0",
          "optional": true
        },
        "vibe-d:data": "*"
      },
      "versions": [
        "trial_as_dependency"
      ]
    },
    {
      "name": "unittest",
      "targetType": "autodetect",
      "excludedSourceFiles": [
        "source/app.d",
        "runner/app.d"
      ],
      "lflags": [
        "-L/opt/homebrew/opt/openssl@3/lib"
      ],
      "sourcePaths": [
        "source",
        "tests"
      ],
      "mainSourceFile": "source/trial/runner.d",
      "importPaths": [
        "source",
        "tests"
      ],
      "dependencies": {
        "fluent-asserts": "~>1.0.0",
        "libdparse": {
          "version": ">=0.20.0",
          "optional": true
        },
        "vibe-d:data": "*"
      }
    }
  ]
}