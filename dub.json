        "source",
        "tests"
      ],
      "mainSourceFile": "source/trial/runner.d",
      "importPaths": [
        "source",
        "tests"
      ],
      "dependencies": {
        "dub": "~>1.29.2",
        "fluent-asserts": "1.0.0-beta.2",
        "libdparse": {
          "version": ">=0.20.0",
          "optional": true
        },
        "vibe-d:data": "~>0.9.5"
